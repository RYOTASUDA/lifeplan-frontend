{
  "printWidth": 100,
  "singleQuote": true,
  "overrides": [
    {
      "files": ["*.ts", "*.tsx"],
      "options": {
        "parser": "typescript"
      }
    },
    {
      "files": ["*.json", "*.jsonc", ".babelrc", ".prettierrc"],
      "options": {
        "parser": "json"
      }
    }
  ]
}
